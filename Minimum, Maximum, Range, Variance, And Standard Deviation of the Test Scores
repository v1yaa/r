scores <- c(85, 90, 78, 92, 88, 76, 95, 89, 84, 91, 
            87, 82, 90, 93, 88, 85, 77, 94, 80, 79)
min_score <- min(scores)
max_score <- max(scores)
range_score <- max_score - min_score
variance_score <- var(scores)
sd_score <- sd(scores)
cat("Minimum Score:", min_score, "\n")
cat("Maximum Score:", max_score, "\n")
cat("Range of Scores:", range_score, "\n")
cat("Variance of Scores:", variance_score, "\n")
cat("Standard Deviation of Scores:", sd_score, "\n")
