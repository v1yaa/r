days <- c("Mon", "Tue", "Wed", "Thu", "Fri", "Sat", "Sun")
barplot(temp, names.arg = days, col = "lightcoral", main = "Max Temperature", xlab = "Days", ylab = "Â°C")
legend("topright", legend = "Temperature (Â°C)", fill = "lightcoral", box.lty = 0)
