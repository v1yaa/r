sales_data <- data.frame(
  Products_Sold = c(50, 60, 45, 70, 90, 30, 80, 100, 55, 75),
  Price = c(20, 18, 22, 15, 12, 25, 14, 10, 19, 13) 
)
cov_sales_price <- cov(sales_data$Products_Sold, sales_data$Price)
cor_sales_price <- cor(sales_data$Products_Sold, sales_data$Price)
cat("Covariance between products sold and price:", cov_sales_price, "\n")
cat("Correlation between products sold and price:", cor_sales_price, "\n")
